t=int(input())
for i in range(t):
    r=list(map(int,input().split()))
    n=r[0]
    p=r[1]
    l=[1 for i in range(n)]
    i=1
    while i<n:
        for j in range(i,n):
            if l[j]==(j+1) or sum(l)==p:
                break
            else:
                l[j]=l[j]+1
        i+=1
    l[0]=p-sum(l)+1
    for i in l:
        print(i,end=" ")
    print(" ")




